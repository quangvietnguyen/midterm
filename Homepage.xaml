<Window x:Class="midterm.Homepage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:midterm"
        mc:Ignorable="d"
        Title="Homepage" Height="716" Width="1434">
    <Grid>
        <DockPanel x:Name="HomepageDock" LastChildFill="False" Margin="0,0,0,654">
            <Menu x:Name="HomepageMenu" Width="1432">
                <MenuItem Header="File" Height="29" Width="51">
                    <MenuItem Header="New"/>
                    <MenuItem Header="Save"/>
                    <MenuItem Header="Quit"/>
                </MenuItem>
                <MenuItem Header="Edit" Height="29" Width="55">
                    <MenuItem Header="Insert"/>
                    <MenuItem Header="Update"/>
                    <MenuItem Header="Delete"/>
                </MenuItem>
                <MenuItem Header="Help" Height="29" Width="62"/>
            </Menu>
        </DockPanel>

    </Grid>
</Window>
